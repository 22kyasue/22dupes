local StarterGui = game:GetService("StarterGui")

local Notifications = {
	"FREE WHITELIST (7 Days Remaining)",
	"FREE WHITELIST (7 Days Remaining)",
}

local TimeBetweenNotifications = 10 --REPLACE 5 WITH THE NUMBER OF SECONDS YOU WANT TO WAIT IN BETWEEN NOTIFICATIONS APPEARING
while true do
	for i = 1, #Notifications do
		local Notification = Notifications[i]
		
		StarterGui:SetCore("SendNotification", {
			Title = "22 Dupe",
			Text = Notification,
		})
		
		wait(TimeBetweenNotifications)
	end
end
--game:service('Players').LocalPlayer:Kick('Ez ez, dm kristina#1111')

end
